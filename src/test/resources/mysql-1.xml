			<property name="searchString" value="Example$"/>
			<property name="replaceString" value="Criteria"/>
		</plugin>
		<jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/mailiqing"
			userId="root" password="111111" />

		<javaModelGenerator targetPackage="mysqlibatis3.pojo" targetProject="gen">
			<property name="enableSubPackages" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="mysqlibatis3.sqlmap" targetProject="gen">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<!-- XMLMAPPER SPRING -->
		<javaClientGenerator type="SPRING" targetPackage="mysqlibatis3.dao" targetProject="gen">
		</javaClientGenerator>


		<table tableName="mlq_product_picture" domainObjectName="Mlq_product_picture"/>
	</context>


